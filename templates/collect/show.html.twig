{% extends 'base.html.twig' %}

{% block title %}Collect{% endblock %}

{% block body %}
    <h1>Collect</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ collect.id }}</td>
            </tr>
            <tr>
                <th>NomDonneur</th>
                <td>{{ collect.NomDonneur }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ collect.Localisation }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ collect.Quantite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_collect_index') }}">back to list</a>

    <a href="{{ path('app_collect_edit', {'id': collect.id}) }}">edit</a>

    {{ include('collect/_delete_form.html.twig') }}
{% endblock %}
